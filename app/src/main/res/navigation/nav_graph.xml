<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/titleScreen">
    <fragment
        android:id="@+id/titleScreen"
        android:name="com.projekt.sobokan.TitleScreen"
        android:label="fragment_title_screen"
        tools:layout="@layout/fragment_title_screen" >
        <action
            android:id="@+id/action_titleScreen_to_mainMenu"
            app:destination="@id/mainMenu" />
    </fragment>
    <fragment
        android:id="@+id/mainMenu"
        android:name="com.projekt.sobokan.MainMenu"
        android:label="fragment_main_menu"
        tools:layout="@layout/fragment_main_menu" >
        <action
            android:id="@+id/action_mainMenu_to_highScores"
            app:destination="@id/highScores" />
        <action
            android:id="@+id/action_mainMenu_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>
    <fragment
        android:id="@+id/highScores"
        android:name="com.projekt.sobokan.HighScores"
        android:label="fragment_high_scores"
        tools:layout="@layout/fragment_high_scores" />
    <fragment
        android:id="@+id/gameFragment"
        android:name="com.projekt.sobokan.GameFragment"
        android:label="fragment_game_fragment"
        tools:layout="@layout/fragment_game_fragment" />
</navigation>